MODULE CPMX1;
(*$I FIBDEF.LIB*)

VAR
  RESULT:INTEGER;

EXTERNAL FUNCTION @BDOS(FUNC,PARM:INTEGER):INTEGER;
 
FUNCTION FILSIZ( VAR F:FIB):INTEGER;
BEGIN
  
  RESULT:=@BDOS(35,ADDR(F.FCB));
  FILSIZ:=256*ORD(F.FCB[34]) + ORD(F.FCB[33])
 
END;

MODEND.
