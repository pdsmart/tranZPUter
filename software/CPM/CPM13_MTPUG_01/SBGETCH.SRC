FUNCTION  SB_GETCH:CHAR;
(* CONVERT MOST WORDSTAR COMMANDS TO SPP EQUIV. FRG-14JUL81 *)
VAR
  CH : CHAR;
  CHI, CHO : INTEGER;
BEGIN
  SB_BIOS_CALL(CONIN,0);
  INLINE("STA / CH);
  CHI := ORD(CH);
  CASE CHI OF		{NEW COMMAND	OLD FUNCTION}
    $04 : CHO := $0C;	{ D		L }
    $05 : CHO := $0B;	{ E		K }
    $06 : CHO := $04;	{ F		D }
    $0A : CHO := $15;	{ J		U }
    $0B : CHO := $16;	{ K		V }
    $0C : CHO := $18;	{ L		X }
    $0F : CHO := $13;	{ O		S }
    $10 : CHO := $14;	{ P		T }
    $13 : CHO := $08;	{ S		H }
    $14 : CHO := $0F;	{ T		O }
    $15 : CHO := $17;	{ U		W }
    $16 : CHO := $06;	{ V		F }
    $17 : CHO := $05;	{ W		E }
    $18 : CHO := $0A;	{ X		J }
    $1F : CHO := $10;	{ -		P }
    ELSE  CHO := CHI;
  END ;  (* CASE *)
  SB_GETCH := CHR(CHO)
END;

