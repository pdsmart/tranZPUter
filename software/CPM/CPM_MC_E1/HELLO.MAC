       .Z80
       ORG    0100H
       LD     DE, MSG
       LD     C, 09H    ; CP/M write string to console call
       CALL   0005H
 
       LD     C,00H    ; CP/M system reset call - shut down
       CALL   0005H

MSG:
       DC   'Hello, World!'
       DB     0Ah
       DB     "$"      ; Line terminator
       END

